{
  "manifest_version": 3,
  "name": "HBdev Bloqueur de domaine publicitaire ",
  "version": "1.0",
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "description": "HBdev est une extension pour bloquer des noms de domaines publicitaires spécifiques développée par HBdev",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "imgs/icon16.png",
      "64": "imgs/icon64.png",
      "128": "imgs/icon128.png"
    }
  },
  "icons": {
    "16": "imgs/icon16.png",
    "64": "imgs/icon64.png",
    "128": "imgs/icon128.png"
  }
}
